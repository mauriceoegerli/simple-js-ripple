{
  "name": "modern-js-ripple",
  "version": "1.0.1",
  "description": "A simple modern ripple effect in javascript",
  "exports": {
    "import": {
      "default": "./dist/index.mjs",
      "types": "./dist/types/index.d.mts"
    },
    "require": {
      "default": "./dist/index.cjs",
      "types": "./dist/types/index.d.cts"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "engines": {
    "node": ">=16.15.0",
    "pnpm": ">=7.1.0"
  },
  "keywords": [
