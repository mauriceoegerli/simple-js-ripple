    "Node"
  ],
  "author": "Maurice Oegerli (modern-js-ripple-support@m-o.dev)",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "0.14.45",
    "rollup": "2.75.6",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-esbuild": "4.9.1",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-pluginutils": "^2.8.2",
    "typescript": "4.7.4"
  }
